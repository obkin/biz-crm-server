    "install": "^0.13.0",
    "ioredis": "^5.4.1",
    "nodemailer": "^6.9.13",
    "npm": "^10.7.0",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
